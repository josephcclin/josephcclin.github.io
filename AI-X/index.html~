            group: [[1, 4], [2, 3, 5]],
            position: [-0.45, 0.4333],
            tp: [-0.45, 0.4333],
            wp: [0.2932, 0.7068],
            r: [-0.1395, 0.1744, 0.0174, -0.0174, 0.0349],
          },
        }

        const getClosest = (val1, val2, target) => {
            if (target - val1 >= val2 - target)
                return 1
            return 0
        }

        const findClosest = (arr, target) => {
          let n = arr.length

          if (target <= arr[0])
              return 0
          if (target >= arr[n - 1])
              return n - 1

          let i = 0, j = n, mid = 0
          while (i < j) {
              mid = parseInt((i + j) / 2)
              if (arr[mid] == target)
                  return mid
              if (target < arr[mid]) {
                  if (mid > 0 && target > arr[mid - 1])
                      return mid + getClosest(arr[mid - 1], arr[mid], target)
                  j = mid
              } else {
                  if (mid < n - 1 && target < arr[mid + 1])
                      return mid + getClosest(arr[mid], arr[mid + 1], target)
                  i = mid + 1
              }
          }
          return mid
        }
        // 1, 2, 3, 4: #d04a5c23 #4a6bd023 #ffc1073b #02798c33 #bababa69
